    <%- include("../partials/header") %>
    <h1 class="position-absolute top-0 start-50 translate-middle-x">My skills as a Developer!</h1>
    <h2 class="mt-5">Development Skills:</h2>    
    <ul>
        <% skills.forEach(function(t) { %>
        <li>
           <a href="/skills/<%= t.id %>"><%= t.skill %></a>
            -
            <%= t.proficiency %>
        </li>
        <% }); %>
    </ul>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
    <a href="/skills/add">Add a skill!</a>
    <%- include("../partials/footer") %>
